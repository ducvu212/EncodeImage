<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.minhd.demoappimagelock">

    <uses-permission android:name="android.permission.PACKAGE_USAGE_STATS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CodeActivity"></activity>
        <activity android:name=".PatternActivity" />
        <activity android:name=".gallery.BaseGalleryActivity"></activity>
        <activity android:name=".gallery.GalleryActivity"></activity>
        <activity android:name=".Main2Activity"
            android:theme="@style/AppThemeMain"></activity>
        <activity android:name=".CustomGallery.CustomGalleryActivity">
            <intent-filter>
                <action android:name="ACTION_PICK" />
                <action android:name="ACTION_MULTIPLE_PICK" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".CustomGallery.ViewActivity"></activity>
        <activity android:name=".Encrypt.EncryptMainActivity"></activity>
        <activity android:name=".Encrypt.ImageActivity"></activity>
    </application>

</manifest>